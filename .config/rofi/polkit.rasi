@import "../themes/rofi.rasi"
@import "combo-modi.rasi"

window {
    width: @width-polkit;
}

prompt {
    background-color: @focused-polkit;
}

element {
    border-color: @focused-polkit;
}

element.selected.normal, element.alternate.active, element.selected.active, element.normal.active {
    background-color: @focused-polkit;
}